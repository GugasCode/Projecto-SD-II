<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>/resources/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
    <managed-bean>
        <managed-bean-name>exerciseBean</managed-bean-name>
        <managed-bean-class>sd.projectii.beans.ExerciseBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>solutionBean</managed-bean-name>
        <managed-bean-class>sd.projectii.beans.SolutionBean</managed-bean-class>
    
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>ucManagedBean</managed-bean-name>
        <managed-bean-class>sd.projectii.beans.UcManagedBean</managed-bean-class>
    
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>userBean</managed-bean-name>
        <managed-bean-class>sd.projectii.beans.UserBean</managed-bean-class>
    
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
</faces-config>
